<script type="text/javascript" src="https://www.google.com/jsapi"></script>

<script type="text/javascript">
       google.load("visualization", "1", { packages: ["corechart"] });
       google.setOnLoadCallback(drawChart);
       function drawChart() {
           $.get('/Home/GetData', {},
   function (data) {
       var tdata = new google.visualization.DataTable();
 
       tdata.addColumn('string', 'Mes');
       tdata.addColumn('number', 'Valor');
       tdata.addColumn('number', 'Despesa');
 
       for (var i = 0; i < data.length; i++) {
           tdata.addRow([data[i].Year, data[i].Salary, data[i].Expense]);
       }
 
       var options = {
           title: "Despesas, Gastos do período"
       };
 
       var chart = new google.visualization.AreaChart(document.getElementById('chart_div'));
       chart.draw(tdata, options);
   });
       }
   </script>

@{
    ViewBag.Title = "DashboardExpensesByDate";
}

<h2>DashboardExpensesByDate</h2>

<div id="chart_div" style="width: 900px; height: 500px;">
      </div>